model Company {
  id               String         @id @default(uuid())
  name             String
  addressId        String?        @unique
  Address          Address?
  createdAt        DateTime       @default(now())
  updatedAt        DateTime       @updatedAt
  SubscriptionDate DateTime       @default(now())
  Clients          Client[]
  FormTemplates    FormTemplate[]
  Reports          Report[]
  Users            User[]
}
