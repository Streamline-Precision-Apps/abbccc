model Company {
  id               String         @id @default(uuid())
  name             String
  address          String
  city             String
  state            String
  zipCode          String
  createdAt        DateTime       @default(now())
  updatedAt        DateTime       @updatedAt
  SubscriptionDate DateTime       @default(now())
  Clients          Client[]
  FormTemplates    FormTemplate[]
  Reports          Report[]
  Jobsites         Jobsite[]
  Users            User[]
}
